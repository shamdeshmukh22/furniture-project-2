<%-include('navbar.ejs')%>
<script src="https://kit.fontawesome.com/7ce8200eeb.js" crossorigin="anonymous"></script>
<div class="container bg-dark  mt-5">

    <div class="row">
        <div class="col-3" style="background-color: #2a2c31;">
           
            <span class="w-100 ms-2 mb-3">
                <img style="height:150px;width: 170px;" class="avatar-img rounded-circle border border-white border-3 shadow ms-5 mt-2 " src="data:image/png;base64," alt="">
            </span>
            <h5 class="text-center text-white mt-4">Sham deshmukh</h5>
            <h6 class="text-center  text-secondary">Shamdeshmukh@gmail.com</h6>
            <ul class="mt-3">

                <li class=" mt-2 text-white">
                    <a class="text-white nav-link" href="/profile"><i class="fa-regular fa-user me-3"></i>My Profile</a>
                </li>
                
               
                <li class=" mt-2 text-white">
                    <a class="nav-link text-white" href="/my_order" ><i class="fa-solid fa-cart-shopping me-3"></i>My orders</a>
                </li>
                <li class=" mt-2 text-white">
                    <a class="nav-link text-white"  href="/cart"><i class="fa-regular fa-heart me-3"></i>Wishlist</a>
                </li>
                <li class=" mt-2 text-white">
                    <a class="nav-link text-white" ><i class="fa-solid fa-gear me-3"></i>Settings</a>
                </li>
                <li class=" mt-2 text-white">
                    <a class="nav-link text-white"><i class="fa-solid fa-trash me-3"></i>Delete Profile</a>
                </li>
                <li class=" mt-2 text-white">
                    <a class="nav-link  text-danger bg-danger-soft-hover" href="/logout"><i class="fas fa-sign-out-alt fa-fw me-2"></i>Sign Out</a>
                </li>
            </ul>
        </div>
        <!-- col -9 part -->
        <div class="col-9 text-white p-3 mb-5" style="background-color: #2a2c31;">
            <div class="overflow-hidden">
                <h6>Complete Your Profile</h6>
                <div class="progress progress-sm bg-success bg-opacity-10">
                    <div class="progress-bar bg-success aos " role="progressbar" data-aos="slide-right" data-aos-delay="200" data-aos-duration="1000" data-aos-easing="ease-in-out" style="width: 85%" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100">
                    <span>85%</span>
                </div>
            </div>
            </div>
            <div class=" text-white p-3 mt-3 " style="background-color: #222529;">
                <ul class="list-inline hstack flex-wrap gap-2 justify-content-between mb-0">
                    <li class="text-white"><a href="" class="text-white"><i class="fa-solid fa-circle-check text-success me-3 p-2 "></i>Verified Email</a></li>
                    <li class="text-white"><a href="" class="text-white"><i class="fa-solid fa-circle-check text-success me-3"></i>Verified Mobile Number</a></li>
                    <li class="text-white"><a href="" class="text-warning" class="text-primary"><i class="fa-solid fa-plus me-3"></i>Complete Basic Info</a></li>
                </ul>
            </div>
            <div class="bg-dark">
                <!-- Card header -->
                <div class="card-header border-bottom">
                    <h4 class="card-header-title">Personal Information</h4>
                </div>

                <!-- Card body START -->
                <div class="card-body">
                    <!-- Form START -->
                    <form class="row g-3">
                        <!-- Profile photo -->
                        <div class="col-12">
                            <label class="form-label">Upload your profile photo<span class="text-danger">*</span></label>
                            <div class="d-flex align-items-center">
                                <label class="position-relative me-4" for="uploadfile-1" title="Replace this pic">
                                    <!-- Avatar place holder -->
                                    <span class="avatar avatar-xl">
                                        <img style="height:80px; width: 100px;" class="avatar-img rounded-circle border border-white border-3 shadow " src="data:image/png;base64," alt="">
                                    </span>
                                </label>
                                <!-- Upload button -->
                                <label class="btn btn-sm btn-primary-soft mb-0" for="uploadfile-1">Change</label>
                                <input id="uploadfile-1" class="form-control d-none" type="file">
                            </div>
                        </div>

                        <!-- Name -->
                        <div class="col-md-6">
                            <label class="form-label">Full Name<span class="text-danger">*</span></label>
                            <input type="text" class="form-control" value="sham deshmukh" placeholder="Enter your full name">
                        </div>

                        <!-- Email -->
                        <div class="col-md-6">
                            <label class="form-label">Email address<span class="text-danger">*</span></label>
                            <input type="email" class="form-control" value="shamdeshmukh@gmail.com" placeholder="Enter your email id">
                        </div>

                        <!-- Mobile -->
                        <div class="col-md-6">
                            <label class="form-label">Mobile number<span class="text-danger">*</span></label>
                            <input type="text" class="form-control" value="9322455289" placeholder="Enter your mobile number">
                        </div>

                        <!-- Nationality -->
                        <div class="col-md-6">
                            <label class="form-label">Nationality<span class="text-danger">*</span></label>
                            <select class="form-select js-choice">
                                <option value="">Select your country</option>
                                <option>USA</option>
                                <option >Paris</option>
                                <option selected>India</option>
                                <option>UK</option>
                            </select>
                        </div>

                        <!-- Date of birth -->
                        <div class="col-md-6">
                            <label class="form-label">Date of Birth<span class="text-danger">*</span></label>
                            <input type="text" class="form-control flatpickr" value="5 july 2004" placeholder="Enter date of birth" data-date-format="d M Y">
                        </div>

                        <!-- Gender -->
                        <div class="col-md-6">
                            <label class="form-label">Select Gender<span class="text-danger">*</span></label>
                            <div class="d-flex gap-4">
                                <div class="form-check radio-bg-light">
                                    <input class="form-check-input" type="radio" name="flexRadioDefault" id="flexRadioDefault1" checked="">
                                    <label class="form-check-label" for="flexRadioDefault1">
                                        Male
                                    </label>
                                </div>
                                <div class="form-check radio-bg-light">
                                    <input class="form-check-input" type="radio" name="flexRadioDefault" id="flexRadioDefault2">
                                    <label class="form-check-label" for="flexRadioDefault2">
                                        Female
                                    </label>
                                </div>
                                <div class="form-check radio-bg-light">
                                    <input class="form-check-input" type="radio" name="flexRadioDefault" id="flexRadioDefault3">
                                    <label class="form-check-label" for="flexRadioDefault3">
                                        Others
                                    </label>
                                </div>
                            </div>
                        </div>

                        <!-- Address -->
                        <div class="col-12">
                            <label class="form-label">Address</label>
                            <textarea class="form-control" rows="3" spellcheck="false">Balikashram Road Ahamdanagar</textarea>
                        </div>

                        <!-- Button -->
                        <div class=" w-100">
                            <button class=" btn btn-primary float-end"> Save Changes  </button>   </div>
                    </form>
                    <!-- Form END -->
                </div>
                <!-- Card body END -->
            </div>
            <div class="card mt-3 mb-3" style="background-color: #191b1d;">
                <!-- Card header -->
                <div class="card-header border-bottom">
                    <h4 class="card-header-title">Update email</h4>
                    <p class="mb-0">Your current email address is <span class="text-warning">example@gmail.com</span></p>
                </div>

                <!-- Card body START -->
                <div class="card-body">
                    <form>
                        <!-- Email -->
                        <label class="form-label">Enter your new email id<span class="text-danger">*</span></label>
                        <input type="email" class="form-control" placeholder="Enter your email id">

                        <div class="text-end mt-3">
                            <a href="#" class="btn btn-primary mb-0">Save Email</a>
                        </div>
                    </form>	
                </div>
                <!-- Card body END -->
            </div>
            <!-- Update email END -->

            <!-- Update Password START -->
            <div class="card " style="background-color: #282829b5;">
                <!-- Card header -->
                <div class="card-header border-bottom">
                    <h4 class="card-header-title">Update Password</h4>
                </div>

                <!-- Card body START -->
                <form class="card-body">
                    <!-- Current password -->
                    <div class="mb-3">
                        <label class="form-label">Current password</label>
                        <input class="form-control" type="password" placeholder="Enter current password">
                    </div>
                    <!-- New password -->
                    <div class="mb-3">
                        <label class="form-label"> Enter new password</label>
                        <div class="input-group">
                            <input class="form-control fakepassword" placeholder="Enter new password" type="password" id="psw-input">
                            <span class="input-group-text p-0 bg-transparent">
                                <i class="fakepasswordicon fas fa-eye-slash cursor-pointer p-2"></i>
                            </span>
                        </div>
                    </div>
                    <!-- Confirm -->
                    <div class="mb-3">
                        <label class="form-label">Confirm new password</label>
                        <input class="form-control" type="password" placeholder="Confirm new password">
                    </div>

                    <div class="text-end">
                        <a href="#" class="btn btn-primary mb-0">Change Password</a>
                    </div>
                </form>
                <!-- Card body END -->
            </div>
        </div>
    </div>
</div>
<br><br><br>
<%-include('footer.ejs')%>

<style>
    li
    {
        list-style-type: none;
    }
</style>